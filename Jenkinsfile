pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'This is build'
            }
        }

        stage('Test') {
            steps {
                echo 'This is test'
            }
        }

        stage('Deploy') {
            steps {
                echo 'This is deploy'
            }
        }

        stage('Verify') {
            steps {
                echo 'This is verify'
            }
        }
    }
}